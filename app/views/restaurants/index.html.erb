<h1>Nuestros Productos</h1>
<% categories = %w[covid guantes sillas andadores] %>
<h2>categorias</h2>

<ul>
<% categories.each do |category| %>
  <li><%= category.capitalize %></li>
<% end %>
</ul>
<h2>Productos</h2>
<ul>
<% @restaurants.each do |id, restaurant| %>
  <li><%= restaurant[:name] %> (<%= restaurant[:precio] %>)</li>
<% end %>
</ul>